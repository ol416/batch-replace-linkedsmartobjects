{
  "id": "batch-replace-linkedsmartobjects.ol416",
  "name": "Batch Replace Linked Smart Objects",
  "version": "1.0.0",
  "main": "index.html",
  "host": {
    "app": "PS",
    "minVersion": "23.3.0",
    "data": {
      "apiVersion": 2
    }
  },
  "manifestVersion": 5,
  "requiredPermissions": {
    "allowCodeGenerationFromStrings": true,
    "clipboard": "readAndWrite",
    "localFileSystem": "fullAccess",
    "launchProcess": {
      "schemes": [
        "http",
        "https"
      ]
    },
    "ipc": {
      "enablePluginCommunication": true
    },
    "network": {
      "domains": "all"
    }
  },
  "entrypoints": [
    {
      "type": "command",
      "id": "showAlert",
      "label": "Show alert"
    },
    {
      "type": "panel",
      "id": "vanilla",
      "minimumSize": {
        "width": 250,
        "height": 200
      },
      "maximumSize": {
        "width": 2000,
        "height": 2000
      },
      "preferredDockedSize": {
        "width": 600,
        "height": 800
      },
      "preferredFloatingSize": {
        "width": 600,
        "height": 800
      },
      "icons": [
        {
          "width": 32,
          "height": 32,
          "path": "icons/dark.png",
          "scale": [
            1,
            2
          ],
          "theme": [
            "dark",
            "darkest"
          ],
          "species": [
            "generic"
          ]
        },
        {
          "width": 32,
          "height": 32,
          "path": "icons/light.png",
          "scale": [
            1,
            2
          ],
          "theme": [
            "lightest",
            "light"
          ],
          "species": [
            "generic"
          ]
        }
      ],
      "label": {
        "default": "Starter Panel"
      }
    }
  ],
  "icons": [
    {
      "width": 23,
      "height": 23,
      "path": "icons/dark.png",
      "scale": [
        1,
        2
      ],
      "theme": [
        "darkest",
        "dark",
        "medium"
      ]
    },
    {
      "width": 23,
      "height": 23,
      "path": "icons/light.png",
      "scale": [
        1,
        2
      ],
      "theme": [
        "lightest",
        "light"
      ]
    }
  ]
}